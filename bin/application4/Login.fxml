<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application4.LoginController">
   <center>
      <AnchorPane prefHeight="273.0" prefWidth="314.0" style="-fx-background-color: BLACK;" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="104.0" layoutY="38.0" prefHeight="34.0" prefWidth="106.0" style="-fx-alignment: CENTER;" text="Login" textFill="WHITE">
               <font>
                  <Font name="American Typewriter" size="19.0" />
               </font>
            </Label>
            <Label layoutX="30.0" layoutY="103.0" text="User Name" textFill="WHITE">
               <font>
                  <Font name="American Typewriter" size="14.0" />
               </font></Label>
            <Label layoutX="30.0" layoutY="152.0" text="Password" textFill="WHITE">
               <font>
                  <Font name="American Typewriter" size="14.0" />
               </font></Label>
            <Button fx:id="btnlogin" layoutX="118.0" layoutY="204.0" mnemonicParsing="false" onAction="#Login" prefHeight="27.0" prefWidth="79.0" style="-fx-background-color: #0087D4;" text="Log in" textFill="WHITE">
               <font>
                  <Font name="American Typewriter" size="15.0" />
               </font>
               <effect>
                  <Lighting>
                     <bumpInput>
                        <Shadow />
                     </bumpInput>
                     <light>
                        <Light.Distant />
                     </light>
                  </Lighting>
               </effect></Button>
            <ComboBox fx:id="comb" layoutX="122.0" layoutY="99.0" prefHeight="27.0" prefWidth="162.0" promptText="Select User" style="-fx-background-color: #6F6F6F #6F6F6F;" />
            <PasswordField fx:id="txtpass" layoutX="123.0" layoutY="148.0" style="-fx-background-color: #6F6F6F;" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
